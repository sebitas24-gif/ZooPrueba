{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ZooPruebaContext": "Host= dpg-d4nndjbe5dus7388hkpg-a.oregon-postgres.render.com;Port=5432;Database= torneodb;Username= admin;Password= ujj82rTxe5M3QTq8Vbza8E2DFjbtN2sp;Ssl Mode=Require;Trust Server Certificate=true;"

  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5050"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/Zoo_Prueba_.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message:lj}. {Exception}{NewLine}",
          "restrictToMinimumLevel": "Information",
          "fileSizeLimitBytes": 10485760,
          "rollOnFileSizeLimit": true,
          "shared": true,
          "buffered": false

        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/Zoo_Prueba_critical_.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message:lj}. {Exception}{NewLine}",
          "restrictedToMinimumLevel": "Error",
          "fileSizeLimitBytes": 10485760,
          "rollOnFileSizeLimit": true,
          "shared": true,
          "buffered": false
        }
      }

    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ]
  }
 

}